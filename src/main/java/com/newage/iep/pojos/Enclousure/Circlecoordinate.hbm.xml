<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newage.iep.pojos.Enclousure.Circlecoordinate" table="circlecoordinate" >
        <id name="id" type="java.lang.String">
            <column name="id"/>
            <generator class="uuid"/>
        </id>
        <property name="circleLng" column="CircleLng"/>
        <property name="circleLat" column="CircleLat"/>
        <property name="enclosureId" column="EnclosureId"/>
        <property name="circleRadius" column="CircleRadius"/>
    </class>
</hibernate-mapping>