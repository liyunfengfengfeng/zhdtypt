<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newage.iep.pojos.Enclousure.Enclosure" table="enclosure" >
        <id name="id" type="java.lang.String">
            <column name="id"/>
            <generator class="uuid"/>
        </id>

        <property name="enclosureName" column="EnclosureName"/>
        <property name="attendanceId" column="AttendanceId"/>
        <property name="coordinateType" column="CoordinateType"/>
        <property name="modifyId" column="ModifyId"/>
        <property name="modifyDate" column="ModifyDate"/>
        <property name="status" column="status"/>
        <property name="descriptions" column="Descriptions"/>
    </class>
</hibernate-mapping>