<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.newage.iep.pojos.Enclousure.Polygoncoordinate" table="polygoncoordinate" >
        <id name="id" type="java.lang.String">
            <column name="id"/>
            <generator class="uuid"/>
        </id>
        <property name="polygonLng" column="PolygonLng"/>
        <property name="polygonLat" column="PolygonLat"/>
        <property name="enclosureId" column="EnclosureId"/>
    </class>
</hibernate-mapping>