<?xml version="1.0" encoding="UTF-8"?>
<ehcache>

    <diskStore path="java.io.tmpdir"/>
    
    <!-- default cache expires items after 30 mins -->
    <defaultCache
        maxElementsInMemory="3000"
        eternal="false"
        timeToIdleSeconds="1200"
        timeToLiveSeconds="1200"
        overflowToDisk="true" />

